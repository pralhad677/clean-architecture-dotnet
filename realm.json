{
  "realm": "myrealm",
  "enabled": true,
  "clients": [
    {
      "clientId": "my-client",
      "enabled": true,
      "protocol": "openid-connect",
      "publicClient": true,
      "redirectUris": [
        "http://localhost:8080/*"
      ]
    }
  ],
  "users": [
    {
      "username": "user",
      "enabled": true,
      "emailVerified": true,
      "credentials": [
        {
          "type": "password",
          "value": "password"
        }
      ]
    }
  ]
}
